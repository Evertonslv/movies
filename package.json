  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "tsx watch src/server.ts",
    "build": "rimraf dist && tsup src",
    "typeorm": "typeorm-ts-node-esm",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ormconfig.ts",
